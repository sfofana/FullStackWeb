package com.app.demo.ex;

public class Person {

	private String name = "Sufyan";
	private int age = 27;
	private String height = "5 foot 10 inches";
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getHeight() {
		return height;
	}
	public void setHeight(String height) {
		this.height = height;
	} 	
	
}
